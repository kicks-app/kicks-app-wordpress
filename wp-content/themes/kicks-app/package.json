{
  "name": "twentysixteen",
  "version": "0.0.1",
  "description": "Bootstrap Sass TwentySixteen",
  "main": "index.js",
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.4",
    "tether": "^1.3.7"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-auto": "^1.1.0",
    "babel-preset-es2015-node4": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "browserify-shim": "^3.8.12",
    "copy": "^0.3.0",
    "css-loader": "^0.25.0",
    "eslint": "^1.9.0",
    "expose-loader": "^0.7.1",
    "font-awesome": "^4.6.3",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.10.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  },
  "scripts": {
    "build-css": "node-sass --source-map true --output-style expanded ./css/index.scss ./css/index-bundle.css --include-path ./node_modules ",
    "watch-css": "npm run build-css -- --watch",
    "build-js": "webpack js/index.js js/index-bundle.js",
    "watch-js": "npm run build-js -- --watch",
    "build": "npm run build-css && npm run build-js",
    "watch": "npm run watch-css | npm run watch-js"
  },
  "keywords": [
    "wordpress-theme"
  ],
  "author": "Rafael Nowrotek",
  "license": "ISC"
}
